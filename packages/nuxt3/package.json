{
  "name": "@intlify/nuxt3",
  "version": "0.1.5",
  "description": "Nuxt3 module for vue-i18n-next",
  "homepage": "https://github.com/intlify/packages/nuxt3/tree/main#readme",
  "bugs": {
    "url": "https://github.com/inlitify/nuxt3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/nuxt3.git",
    "directory": "packages/nuxt3"
  },
  "license": "MIT",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./dist/runtime/*": "./dist/runtime/*",
    "./runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist/*.cjs",
    "dist/*.mjs",
    "dist/*.d.ts",
    "dist/runtime/*.cjs",
    "dist/runtime/*.mjs",
    "dist/runtime/*.d.ts"
  ],
  "scripts": {
    "build": "unbuild"
  },
  "dependencies": {
    "@intlify/shared": "beta",
    "@intlify/vite-plugin-vue-i18n": "next",
    "@intlify/vue-i18n-loader": "next",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.0-27249044.7d918e1",
    "pathe": "^0.2.0",
    "vue-i18n": "beta"
  },
  "devDependencies": {
    "unbuild": "^0.5.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
